#class {bot} {kill}
#class {bot} {open}

#var no_loop 1
#var no_home 1
#var {no_player} {1}

#var {botpath} {guildsetup;enter;e;e;n;e;e;n;w;w;nw;w;w;ne;e;e;n;w;w;n;challenge}

#act {^You wouldn't want to disrespect the arena by leaving} {
	#path move -1;
	#map undo;
	challenge;
	look
}

#act {^=M= {An Aspiring Knight|A 2nd Class Knight|A Knighted Officer|A Knight Commander|A Knight Grand Commander|A Knight's Grand Champion}} {
	#var mobname elf;
	_check_mob
}

#class {bot} {close}
